{
    "id": "nodebb-plugin-u77-connect",
    "name": "NodeBB U77 Connect",
    "url": "https://github.com/vcing/nodebb-plugin-u77-connect.git",
    "library": "./library.js",
    "hooks": [{
            "hook": "static:app.load",
            "method": "init"
        },
        {
            "hook": "action:auth.overrideLogin",
            "method": "auth"
        },
        {
            "hook": "filter:router.page",
            "method": "routeFilter"
        },
        {
            "hook": "filter:auth.init",
            "method": "getStrategy"
        },
        {
            "hook": "filter:register.check",
            "method": "registerCheck"
        },
        {
            "hook": "filter:register.complete",
            "method": "registerComplete"
        }
    ],
    "templates": "./templates",
    "scripts": [
        "client.js"
    ]
}